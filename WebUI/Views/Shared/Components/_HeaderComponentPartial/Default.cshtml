@model List<SliderTitle>

<div class="container-fluid header bg-white p-0">
    <div class="row g-0 align-items-center flex-column-reverse flex-md-row">
        <div class="col-md-6 p-5 mt-lg-5">
            <h1 class="display-5 animated fadeIn mb-4"> <span class="text-primary">Muhteşem Evler , Yatırımlık Tarla ve Araziler İçin</span></h1>
            <p class="animated fadeIn mb-4 pb-2">
               Doğru Adrestesiniz
            </p>
            <a href="" class="btn btn-primary py-3 px-5 me-3 animated fadeIn">Get Started</a>
        </div>
        <div class="col-md-6 animated fadeIn">
            <div class="owl-carousel header-carousel">
                @foreach (var item in Model)
                {
                    <div class="owl-carousel-item">
                        <img class="img-fluid" src="~/myimages/slidertitle/@item.ImageUrl" style="width:1000px" height="1000px" alt="">
                    </div>
                }
                
            </div>
        </div>
    </div>
</div>
