@model GetAllProductViewModel
<div class="container-xxl py-5">
    <div class="container">
        <div class="row g-0 gx-5 align-items-end">
            <div class="col-lg-6">
                <div class="text-start mx-auto mb-5 wow slideInLeft" data-wow-delay="0.1s">
                    <h1 class="mb-3">İlanların Listesi</h1>
                    <p>Detaylı bilgi isterseniz Ürün detaylarını görebilirsiniz</p>
                </div>
            </div>
            <div class="col-lg-6 text-start text-lg-end wow slideInRight" data-wow-delay="0.1s">
                <ul class="nav nav-pills d-inline-flex justify-content-end mb-5">
                    <li class="nav-item me-2">
                        <a class="btn btn-outline-primary" data-bs-toggle="pill" href="#tab-1">Karışık</a>
                    </li>
                    <li class="nav-item me-2">
                        <a class="btn btn-outline-primary" data-bs-toggle="pill" href="#tab-2">Satılık</a>
                    </li>
                    <li class="nav-item me-2">
                        <a class="btn btn-outline-primary" data-bs-toggle="pill" href="#tab-3">Kiralık</a>
                    </li>
                    <li class="nav-item me-2">
                        <a class="btn btn-outline-primary" data-bs-toggle="pill" href="#tab-4">Arsa</a>
                    </li>
                    <li class="nav-item me-0">
                        <a class="btn btn-outline-primary" data-bs-toggle="pill" href="#tab-5">Tarla</a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="tab-content">
            <div id="tab-1" class="tab-pane fade show p-0 active">
                <div class="row g-4">
                    @foreach (var item in Model.ForInProduct)
                    {
                        
                        <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
                            <div class="property-item rounded overflow-hidden">
                                <div class="position-relative overflow-hidden">
                                    <a href="/Product/ProductDetail/@item.ProductId"><img class="img-fluid" src="~/myimages/product/@item.ImageUrl" alt=""></a>
                                    <div class="bg-primary rounded text-white position-absolute start-0 top-0 m-4 py-1 px-3">@item.SubCategoryName</div>
                                    <div class="bg-white rounded-top text-primary position-absolute start-0 bottom-0 mx-4 pt-1 px-3">@item.CategoryName</div>
                                </div>
                                <div class="p-4 pb-0">
                                    <h5 class="text-primary mb-3">@item.Price</h5>
                                    <a class="d-block h5 mb-2" href="">@item.Title</a>
                                    <p><i class="fa fa-map-marker-alt text-primary me-2"></i>@item.Address</p>
                                </div>
                                <div class="d-flex border-top">
                                    <small class="flex-fill text-center border-end py-2"><i class="fa fa-ruler-combined text-primary me-2"></i>@item.Area</small>
                                    <small class="flex-fill text-center border-end py-2"><i class="fas fa-hospital-alt text-primary me-2"></i>@item.RoomCount</small>
                                    <small class="flex-fill text-center py-2"><i class="fab fa-pagelines text-primary me-2"></i>@item.HouseAge</small>
                                </div>
                            </div>
                        </div>
                       
                    }


                </div>
            </div>
            <div id="tab-2" class="tab-pane fade show p-0 active">
                <div class="row g-4">
                    @foreach (var item in Model.ForSellInProduct)
                    {
                        <div class="col-lg-4 col-md-6">
                            <div class="property-item rounded overflow-hidden">
                                <div class="position-relative overflow-hidden">
                                    <a href=""><img class="img-fluid" src="~/myimages/product/@item.ImageUrl" alt=""></a>
                                    <div class="bg-primary rounded text-white position-absolute start-0 top-0 m-4 py-1 px-3">@item.SubCategoryName</div>
                                    <div class="bg-white rounded-top text-primary position-absolute start-0 bottom-0 mx-4 pt-1 px-3">@item.CategoryName</div>
                                </div>
                                <div class="p-4 pb-0">
                                    <h5 class="text-primary mb-3">@item.Price</h5>
                                    <a class="d-block h5 mb-2" href="">@item.Title</a>
                                    <p><i class="fa fa-map-marker-alt text-primary me-2"></i>@item.Address</p>
                                </div>
                                <div class="d-flex border-top">
                                    <small class="flex-fill text-center border-end py-2"><i class="fa fa-ruler-combined text-primary me-2"></i>@item.Area</small>
                                    <small class="flex-fill text-center border-end py-2"><i class="fa fa-bed text-primary me-2"></i>@item.RoomCount</small>
                                    <small class="flex-fill text-center py-2"><i class="fa fa-bath text-primary me-2"></i>@item.HouseAge</small>
                                </div>
                            </div>
                        </div>

                    }


                </div>
            </div>
            <div id="tab-3" class="tab-pane fade show p-0 active">
                <div class="row g-4">
                    @foreach (var item in Model.ForRentInProduct)
